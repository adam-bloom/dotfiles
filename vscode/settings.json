{
  "workbench.colorTheme": "(Actual) Obsidian",
  "editor.fontFamily": "DejaVuSansMono Nerd Font, Menlo, Monaco, 'Courier New', monospace",
  "telemetry.enableTelemetry": false,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "DejaVuSansMono Nerd Font",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "python.languageServer": "Pylance",
  "explorer.confirmDelete": false,
  "editor.wordWrap": "on",
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.wrapTabs": true,
  "[python]": {
    "gitlens.codeLens.symbolScopes": ["!Module"],
    "editor.wordBasedSuggestions": "off",
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "jupyter.runStartupCommands": [
    "import sys; sys.path.insert(0, '${workspaceRoot}')"
  ],
  "jupyter.askForKernelRestart": false,
  "jupyter.textOutputLimit": 50000,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "files.insertFinalNewline": true,
  "go.toolsManagement.autoUpdate": true,
  "python.sortImports.args": [
    "--profile",
    "black",
    "--line-length",
    "120",
    "--multi-line",
    "3"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "files.associations": {
    "*.hcl": "terraform"
  },
  "yaml.customTags": ["!encrypted/pkcs1-oaep scalar", "!vault scalar"],
  "ansible.validate.executablePath": "ansible-lint",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "files.watcherExclude": {
    "": true
  },
  "python.testing.pytestEnabled": false,
  "redhat.telemetry.enabled": false,
  "eslint.validate": ["javascript"],
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[javascript][javascriptreact][mdx][typescript][typescriptreact][scss][markdown][json][jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.autoSave": "onFocusChange",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "vscode-yaml-sort.useCustomSortRecursively": true,
  "vscode-yaml-sort.customSortKeywords_2": ["name"],
  "notebook.output.textLineLimit": 50000,
  "jupyter.debugJustMyCode": false,
  "diffEditor.codeLens": true,
  "git.mergeEditor": false,
  "plantuml.jar": "/opt/homebrew/Cellar/plantuml/1.2024.4/libexec/plantuml.jar",
  "python.formatting.provider": "black",
  "window.zoomLevel": 1,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "playwright.showTrace": false,
  "playwright.env": {},
  "python.testing.promptToConfigure": false,
  "playwright.reuseBrowser": false,
  "docker.extension.enableComposeLanguageServer": false,
  "mcpServers": {},
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "claudeCode.environmentVariables": [
    { "name": "CLAUDE_CODE_USE_BEDROCK", "value": "1" },
    { "name": "AWS_REGION", "value": "us-east-1" },
    { "name": "AWS_PROFILE", "value": "qa-standard" },
    {
      "name": "ANTHROPIC_MODEL",
      "value": "us.anthropic.claude-opus-4-5-20251101-v1:0"
    },
    {
      "name": "ANTHROPIC_SMALL_FAST_MODEL",
      "value": "us.anthropic.claude-sonnet-4-5-20250929-v1:0"
    },
    { "name": "CLAUDE_CODE_MAX_OUTPUT_TOKENS", "value": "8192" },
    { "name": "MAX_THINKING_TOKENS", "value": "1024" }
  ],
  "github.copilot.enable": {
    "*": false
  },
  "chat.disableAIFeatures": true,
  "claudeCode.preferredLocation": "panel"
}
